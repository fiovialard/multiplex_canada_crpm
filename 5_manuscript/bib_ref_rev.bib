   note = {Accession Number: 2700978; OCLC: 1232140127; Language: English},
   publisher = {Chapman and Hall/CRC},
   title = {Medical Risk Prediction Models : With Ties to Machine Learning},
   url = {https://proxy.library.mcgill.ca/login?url=https://search.ebscohost.com/login.aspx?direct=true&db=nlebk&AN=2700978&scope=site},
