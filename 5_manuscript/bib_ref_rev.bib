   journal = {J Int Assoc Provid AIDS Care},
   keywords = {*Coinfection,*HIV Infections/complications/epidemiology,*Hepatitis C/epidemiology,Ghana,Ghana/epidemiology,HCV/HIV coinfection,Hiv,Humans,Male,Middle Aged,Prevalence,Seroepidemiologic Studies,clinical prediction score,hepatitis C prevalence,targeted screening},
   note = {2325-9582Torpey, KwasiOgyiri, LilyOrcid: 0000-0002-4523-9328Cuylaerts, VickyAgyeman, SethAgyei-Nkansah, AdwoaBuyze, JozefienCommey, Joseph OliverLynen, LutgardeDe Weggheleire, AnjaOrcid: 0000-0001-9377-4766Journal ArticleResearch Support, Non-U.S. Gov'tUnited States2021/06/02J Int Assoc Provid AIDS Care. 2021 Jan-Dec;20:23259582211022469. doi: 10.1177/23259582211022469.},
